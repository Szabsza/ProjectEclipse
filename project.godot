config_version=5

[application]

config/name="Project Eclipse"
run/main_scene="res://interface/main_menu/MainMenu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

InteractionManager="*res://autoloads/interaction/InteractionManager.tscn"
TravelManager="*res://autoloads/travel/TravelManager.tscn"
StatusManager="*res://autoloads/status/StatusManager.tscn"
HotBarManager="*res://autoloads/hotbar/HotBarManager.tscn"
SaveManager="*res://autoloads/save/SaveManager.tscn"
LoadManager="*res://autoloads/load/LoadManager.tscn"
WorldManager="*res://autoloads/world/WorldManager.tscn"
PlayerManager="*res://autoloads/player/PlayerManager.tscn"
TransitionManager="*res://autoloads/transition/TransitionManager.tscn"
BuffManager="*res://autoloads/buff/BuffManager.tscn"
SceneManager="*res://autoloads/scene/SceneManager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/scale=2.01

[dotnet]

project/assembly_name="Project Eclipse"

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Assets/": "gray",
"res://autoloads/": "blue",
"res://interface/": "green",
"res://items/": "red"
}

